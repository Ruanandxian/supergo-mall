<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.mapper.AreasMapper">
    <select id="getListByCityId" parameterType="string" resultType="com.mapper.pojo.Areas">
        SELECT * from tb_areas where cityid = #{cityid}
    </select>
</mapper>